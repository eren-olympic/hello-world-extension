{
  "manifest_version": 3,
  "name": "Input Field Filler",
  "version": "1.0",
  "description": "Detects all input fields on a page and fills them with 'test'",
  "side_panel": {
    "default_path": "panel.html"
  },
  "action": {
    "default_title": "開啟欄位填寫器"
  },
  "permissions": ["activeTab", "scripting", "tabs", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  }
}
  